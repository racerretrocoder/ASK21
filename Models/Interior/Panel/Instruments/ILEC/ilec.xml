  <animation>
  <type>pick</type>
  <object-name>ignitionswitch</object-name>
  <action>
	  <button>0</button>
    <binding>
      <command>property-cycle</command>
      <property>/controls/engines/engine/magnetos</property>
      <value>3</value>
      <value>0</value>
    </binding>
  </action>
  </animation>

   <animation>
  <type>rotate</type>
  <object-name>ignitionswitch</object-name>
	<property>/controls/engines/engine/magnetos</property>
  <factor>-20</factor>
  <center>
   <x-m>0.00147</x-m>
   <y-m>0.027</y-m>
   <z-m>-0.01747</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
  <animation>
  <type>pick</type>
  <object-name>propes</object-name>
  <action>
	  <button>0</button>
	<binding>
		<command>property-toggle</command>
		<property>/controls/engines/engine/extend-propeller</property>
	</binding>   
  </action>
  </animation>

   <animation>
  <type>rotate</type>
  <object-name>propes</object-name>
	<property>controls/engines/engine/extend-propeller-pos</property>
  <factor>-60</factor>
  <center>
   <x-m>0.003</x-m>
   <y-m>0.027</y-m>
   <z-m>0.011</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 
 <!--RPM Lights--> 
 <animation>
  <type>material</type>
  <condition>
	  <greater-than>
		<property>/engines/engine/engine-rpm</property>
		<value>3000</value>
	</greater-than>
	<less-than-equals>
		<property>/engines/engine/engine-rpm</property>
		<value>7000</value>
	</less-than-equals>
  </condition>
  <object-name>rpm.green</object-name>
  <emission>
        <red>0</red>
	<green>1</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 <animation>
  <type>material</type>
  <condition>
	  <greater-than>
		<property>/engines/engine/engine-rpm</property>
		<value>7000</value>
	</greater-than>
	<less-than-equals>
		<property>/engines/engine/engine-rpm</property>
		<value>7700</value>
	</less-than-equals>
  </condition>
  <object-name>rpm.yellow</object-name>
  <emission>
        <red>1</red>
        <green>1</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 <animation>
  <type>material</type>
  <condition>
	  <greater-than>
		<property>/engines/engine/engine-rpm</property>
		<value>7800</value>
	</greater-than>
  </condition>
  <object-name>rpm.red</object-name>
  <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 
 <!--Warning Light (red)-->
 <!--Ignition Off while starting--> 
 <animation>
  <type>material</type>
  <condition>
	<equals>
		<property>/controls/engines/engine/magnetos</property>
		<value>0</value>
	</equals>
	<property>/controls/engines/engine/starter</property>
  </condition>
  <object-name>ignitionoff</object-name>
  <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 <!--Propeller not fully extended-->
 <animation>
  <type>material</type>
  <condition>
	<greater-than>
		<property>sim/multiplay/generic/float[10]</property>
		<value>0</value>
	</greater-than>
	<less-than>
		<property>sim/multiplay/generic/float[10]</property>
		<value>1</value>
	</less-than>
  </condition>
  <object-name>propnfe</object-name>
  <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 <!--ECU fail-->
 <animation>
  <type>material</type>
  <condition>
	<greater-than>
		<property>engines/engine/ecu-fail</property>
		<value>0</value>
	</greater-than>
  </condition>
  <object-name>ecu</object-name>
  <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 <!--Generator warning-->
 <animation>
  <type>material</type>
  <condition>
	  <and>
		<equals>
			<property>sim/multiplay/generic/float[10]</property>
			<value>1</value>
		</equals>
		<or>
			<less-than>
				<property>engines/engine/engine-rpm</property>
				<value>2000</value>
			</less-than>
			<less-than>
				<property>systems/electrical/eng-volts</property>
				<value>12.8</value>
			</less-than>
		</or>
	</and>			
  </condition>
  <object-name>genwarn</object-name>
  <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 
 <!--Green: correct state-->
 <!--Propeller fully extended-->
 <animation>
  <type>material</type>
  <condition>
	<equals>
		<property>sim/multiplay/generic/float[10]</property>
		<value>1</value>
	</equals>
  </condition>
  <object-name>propfe</object-name>
  <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 <!--Propeller not fully retracted-->
 <animation>
  <type>material</type>
  <condition>
	<equals>
		<property>sim/multiplay/generic/float[10]</property>
		<value>0</value>
	</equals>
  </condition>
  <object-name>propfr</object-name>
  <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
        <factor-prop>/controls/electric/battery-switch</factor-prop>
    </emission>
 </animation>
 
