<?xml version="1.0"?>
<PropertyList>
    <path>panelF-mi.ac</path>
    <!--COCKPIT-->
    <!--FRONT INSTRUMENTS-->
    
    <!--57mm turnslip-->
    <model>
        <name>turnslip</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/57mm_turnslip/57mm_turnslip.xml</path>
        <offsets>
            <x-m>-3.325</x-m>
            <y-m>-0.03</y-m>
            <z-m>0.74</z-m>
            <pitch-deg>-4</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>wind</name>
        <path>Instruments/windcompass/windcompass.xml</path>
        <offsets>
            <x-m>-3.328</x-m>
            <y-m>-0.03</y-m>
            <z-m>0.74</z-m>
            <pitch-deg>-4</pitch-deg>
        </offsets>
    </model>
    <animation>
	    <type>select</type>
	    <object-name>wind</object-name>
	    <condition>
		    <property>/sim/model/windcompass</property>
	    </condition>
    </animation>
    <animation>
	    <type>select</type>
	    <object-name>turnslip</object-name>
	    <condition>
		    <not>
			<property>/sim/model/windcompass</property>
		</not>
	    </condition>
    </animation>
    
