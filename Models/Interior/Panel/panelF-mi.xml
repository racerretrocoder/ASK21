        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/switches/masterswitch.xml</path>
        <offsets>
            <x-m>-3.318</x-m>
            <y-m>-0.092</y-m>
            <z-m>0.63</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>turnslip_switch</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/switches/turnslipswitch.xml</path>
        <offsets>
            <x-m>-3.318</x-m>
            <y-m>-0.068</y-m>
            <z-m>0.63</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>

    <!--Canopy Jettison Handle-->
    <animation>
        <type>slider</type>
        <object-name>canopyjettison</object-name>
        <drag-direction>horizontal</drag-direction>
        <action>
            <binding>
                <condition>
                    <equals>
                    <property>sim/multiplay/generic/float[1]</property>
                    <value>0</value>
                </equals>
                </condition>
                <command>property-adjust</command>
                <property>controls/canopy/lock-jettison</property>
                <factor>-0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>0</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>canopyjettison</tooltip-id>
                <label>Canopy JETTISON LOCK: %s</label>
                <mapping>open-close</mapping>
                <property>controls/canopy/lock-jettison</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>canopyjettison</object-name>
        <property>controls/canopy/lock-jettison</property>
        <factor>-90</factor>
        <axis>
            <x1-m>-3.42562</x1-m>
            <y1-m>0.0</y1-m>
            <z1-m>0.79201</z1-m>
            <x2-m>-3.42981</x2-m>
            <y2-m>0.0</y2-m>
            <z2-m>0.80948</z2-m>
        </axis>
    </animation>
    <!--Cockpit Shader-->
    <effect>
        <inherits-from>Aircraft/ASK21/Models/Effects/interior-shadow</inherits-from>
        <object-name>panelF1</object-name>
        <object-name>panelF2</object-name>
        <object-name>alt</object-name>
        <object-name>asi</object-name>
        <object-name>vario</object-name>
        <object-name>vario2</object-name>
        <object-name>com</object-name>
        <object-name>compass</object-name>
        <object-name>turnslip</object-name>
    </effect>
</PropertyList>
